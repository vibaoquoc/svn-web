[% WHILE branchpoints.size && branchpoints.first.srcrev >= rev %]
[% b = branchpoints.shift %]
branchpoint for
[% IF isdir %]
<a href="[% script %]/[% repos %]/browse[% b.dst %]?rev=[% b.dstrev %]">[% b.dst %]</a>:[% b.dstrev %]<br/>
