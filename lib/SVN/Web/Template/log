[% END %]

[% END %]
[% UNLESS isdir || loop.count == loop.size%]
[% prev = loop.count %]
<a href="[% script %]/[% repos %]/diff[% path %]?rev1=[% revs.$prev.rev %]&rev2=[% rev %]">[%|l%](diff with previous)[%END%]</a><br/>
